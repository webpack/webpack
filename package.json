    "mini-css-extract-plugin": "^2.9.0",
    "mini-svg-data-uri": "^1.2.3",
    "node-gyp": "^11.2.0",
    "nyc": "^17.1.0",
    "open-cli": "^8.0.0",
    "prettier": "^3.6.0",
    "prettier-2": "npm:prettier@^2",
    "pretty-format": "^30.0.2",
    "pug": "^3.0.3",
    "pug-loader": "^2.4.0",
    "raw-loader": "^4.0.1",
    "react": "^19.0.0",
