    "@codspeed/core": "^5.1.0",
    "@types/glob-to-regexp": "^0.4.4",
    "@types/graceful-fs": "^4.1.9",
    "@types/jest": "^30.0.0",
    "@types/mime-types": "^2.1.4",
    "@types/neo-async": "^2.6.7",
    "@types/node": "^25.1.0",
    "@types/xxhashjs": "^0.2.4",
    "assemblyscript": "^0.28.9",
    "babel-loader": "^10.0.0",
